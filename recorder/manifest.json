{
  "manifest_version": 2,
  "name": "Testgram Tech Check Plugin",
  "description": "Capture recorder data to the local computer",
  "version": "1.0",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "js": [
        "recorder.js"
      ]
    }
  ]
}
